{
  "name": "galaxygames/ovommand",
  "description": "Add command overloads",
  "type": "library",
  "minimum-stability": "dev",
  "require": {
    "muqsit/simple-packet-handler": "dev-pm5",
    "pocketmine/pocketmine-mp": "^5.6.0",
    "friendsofphp/php-cs-fixer": "^3.4",
    "phpstan/phpstan": "^1.2",
    "sof3/pharynx": "^0.3.0"
  },
  "autoload": {
    "classmap": [
      "src"
    ]
  },
  "extra": {
    "virion": {
      "spec": "3.0",
      "namespace-root": "galaxygames\\ovommand"
    }
  },
  "scripts": {
    "analyze": "vendor/bin/phpstan",
    "build": "php -dphar.readonly=0 vendor/bin/pharynx -i=. -c -p=Ovommand.phar"
  }
}

{
  "name": "galaxygames/ovommand",
  "description": "a normal framework virion designed to parse command data for PocketMine - MP",
  "type": "library",
  "license": "GPL-3.0",
  "minimum-stability": "dev",
  "require": {
    "muqsit/simple-packet-handler": "^0.1.5",
    "php": "^8.2"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.0",
    "pocketmine/pocketmine-mp": "^5.27.1"
  },
  "autoload": {
    "classmap": [
      "src",
      "shared"
    ]
  },
  "extra": {
    "virion": {
      "spec": "3.1",
      "namespace-root": "galaxygames\\ovommand",
      "shared-namespace-root": "shared\\galaxygames\\ovommand"
    }
  },
  "scripts": {
    "analyze": "vendor/bin/phpstan"
  }
}
